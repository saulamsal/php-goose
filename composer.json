{
    "name": "saulamsal/php-goose",
    "type": "library",
    "description": "Readability / Html Content / Article Extractor & Web Scrapping library written in PHP",
    "keywords": ["scraper", "http", "extractor", "text", "content", "scraping", "website", "readability"],
    "homepage": "https://github.com/saulamsal/php-goose",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Andrew Scott",
            "email": "andrew@andrewscott.net.au"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "ext-mbstring": "*",
        "ext-libxml": "*",
        "lib-libxml": ">=2.7.7",
        "guzzlehttp/guzzle": "^7.0",
        "jakeasmith/http_build_url": "^1.0",
        "scotteh/php-dom-wrapper": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Goose\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goose\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
